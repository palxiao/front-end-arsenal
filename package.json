{
  "name": "root",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "cd ./bin && node buildAll.js",
    "add": "node ./bin/add.js",
    "api": "node ./bin/makeApi.js",
    "build:some": "node ./bin/buildSome.js",
    "c": "git add . && git-cz",
    "clog": "conventional-changelog --config ./node_modules/vue-cli-plugin-norm/lib/log -i CHANGELOG.md -s -r 0 && commit-log",
    "commit": "git add . && git-cz",
    "dev": "cd ./demo && npm run docs:dev",
    "docs:build": "cd ./demo && npm run docs:build",
    "version": "lerna version",
    "bs": "lerna bootstrap",
    "publish": "lerna publish",
    "init": "lerna init"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.13",
    "@babel/generator": "^7.13.9",
    "@babel/parser": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/template": "^7.12.13",
    "@babel/traverse": "^7.13.13",
    "@babel/types": "^7.13.14",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vuese/markdown-render": "^2.11.3",
    "@vuese/parser": "^2.10.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "css-loader": "^5.2.0",
    "eslint": "^7.29.0",
    "eslint-config-alloy": "3",
    "eslint-plugin-vue": "^7.12.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "husky": "^3.0.9",
    "inquirer": "^7.3.3",
    "json-templater": "^1.2.0",
    "lerna": "^4.0.0",
    "less": "^4.1.1",
    "less-loader": "^6.2.0",
    "ora": "^5.4.0",
    "parallel-webpack": "^2.6.0",
    "right-pad": "^1.0.1",
    "sass": "^1.32.8",
    "sass-loader": "^9.0.2",
    "style-loader": "^2.0.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue-cli-plugin-norm": "~1.2.12",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.28.0",
    "webpack-merge": "^5.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-norm/lib/cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "javascript",
    "vue2",
    "component"
  ],
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/palxiao/front-end-arsenal"
  }
